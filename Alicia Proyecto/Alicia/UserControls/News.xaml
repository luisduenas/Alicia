            </Grid>
        </PivotItem>
        <PivotItem>
            <Grid HorizontalAlignment="Center">
                <TextBlock Name ="tBNew4" FontSize="45" Foreground="#FF289DB6" HorizontalAlignment="Center"/>
            </Grid>
        </PivotItem>
        <PivotItem>
            <Grid>
                <TextBlock Name ="tBNew5" FontSize="45" Foreground="#FF289DB6" HorizontalAlignment="Center"/>
